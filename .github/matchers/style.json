{
    "problemMatcher": [
      {
        "owner": "rubocop-error",
        "severity": "error",
        "pattern": [
          {
            "regexp": "^(.+):(\\d+):(\\d+): C: ((.+): .+)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "message": 4,
            "code": 5
          }
        ]
      },
      {
        "owner": "rubocop-warning",
        "severity": "warning",
        "pattern": [
          {
            "regexp": "^(.+):(\\d+):(\\d+): W: ((.+): .+)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "message": 4,
            "code": 5
          }
        ]
      }
    ]
  }
